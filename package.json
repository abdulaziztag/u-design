{
  "name": "u-design",
  "version": "1.0.4",
  "description": "Ugaz component library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/styles/global.css",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": "https://github.com/abdulaziztag/u-design",
  "scripts": {
    "prebuild": "npm run generate:icons",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext=.ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "storybook": "concurrently \"npm run watch:style-types\" \"npm run generate:icons\" \"storybook dev -p 6006\"",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "optimize:icons": "npx svgo -rf ./src/assets/icons",
    "generate:icons": "npm run optimize:icons && node src/scripts/generate-icons.mjs && eslint ./src/components/Icon/* --fix",
    "build:style-types": "npx typed-scss-modules src/components",
    "watch:style-types": "npx typed-scss-modules src/components --watch"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "keywords": [
    "ui",
    "component",
    "design",
    "ugaz"
  ],
  "author": "Abdulaziz Manopov",
  "license": "ISC",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-webpack5": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "rollup": "^4.34.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "storybook": "^8.5.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "typed-scss-modules": "^8.1.1",
    "typescript": "^5.7.3",
    "autoprefixer": "^10.4.20",
    "typescript-eslint": "^8.21.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.test.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "clsx": "^2.1.1"
  }
}
